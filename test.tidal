    (0.5 <~) $ sound "pitm(3,8)" # n "1" # gain "0.8",
  sound "~ pitcp" # cutoff "22000" # resonance "0.01" # gain "0.9",
  whenmod 7 4 ((# n "1") . (# gain "1.2")) $
